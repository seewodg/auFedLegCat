@base <http://example.org/au/leg/profile/lvstex> .

#
#   lvstex, described as a profile for use with a DCAT Catalog
#
:lvstex
    a prof:Profile ;
    rdfs:label "Livestock Export Profile example only" ;
    # this URI is for DCAT (original) as defined in the DCAT examples
    prof:profileOf <http://example.org/au/leg/catalog/> ;
    prof:hasResource :lvstex-rdf ,
.

:lvstex-rdf
    a prof:ResourceDescriptor ;
    rdfs:label "lvstex RDF vocabulary" ;
    prof:hasRole roles:vocabulary ;
    dct:conformsTo <http://example.org/au/leg/profile/> ;
    dct:format <https://w3id.org/mediatypes/text/turtle> ;
